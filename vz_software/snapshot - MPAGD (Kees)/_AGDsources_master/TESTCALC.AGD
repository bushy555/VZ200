 PRINT " 13 -   1 = "
 LET L = 13
 SUBTRACT 1 FROM L
 DISPLAY L
 AT 3 16
 PRINT " 43 -  31 = "
 LET L = 43
 SUBTRACT 31 FROM L
 DISPLAY L
 AT 4 16
 PRINT " 74 -  62 = "
 LET L = 74
 SUBTRACT 62 FROM L
 DISPLAY L
 AT 5 16
 PRINT "106 -  94 = "
 LET L = 106
 SUBTRACT 94 FROM L
 DISPLAY L
 AT 6 16
 PRINT "140 - 128 = "
 LET L = 140
 SUBTRACT 128 FROM L
 DISPLAY L
 AT 7 16
 PRINT "211 - 199 = "
 LET L = 211
 SUBTRACT 199 FROM L
 DISPLAY L
