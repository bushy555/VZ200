EVENT MAINLOOP1
 CLS
 AT 0 1
 PAPER 1
 INK 2
 PRINT "CALCULATIONS TEST:            "
 AT 1 1
 PRINT "Division:      Multiplication:"

 PAPER 0
 INK 2

;--------------------
; Test divide
;--------------------

 AT 2 1
 PRINT "123 /  2 = "
 LET L = 123
 DIVIDE L BY 2
 DISPLAY L
 AT 3 1
 PRINT "123 /  3 = "
 LET L = 123
 DIVIDE L BY 3
 DISPLAY L
 AT 4 1
 PRINT "123 /  4 = "
 LET L = 123
 DIVIDE L BY 4
 DISPLAY L
 AT 5 1
 PRINT "123 /  5 = "
 LET L = 123
 DIVIDE L BY 5
 DISPLAY L
 AT 6 1
 PRINT "123 / 32 = "
 LET L = 123
 DIVIDE L BY 32
 DISPLAY DOUBLEDIGITS L
 AT 7 1
 PRINT "123 / 64 = "
 LET L = 123
 DIVIDE L BY 64
 DISPLAY DOUBLEDIGITS L

;--------------------
; Test multiply
;--------------------
 AT 2 18
 PRINT "3 x  2 = "
 LET L = 3
 MULTIPLY L BY 2
 DISPLAY TRIPLEDIGITS L
 AT 3 18
 PRINT "3 x  3 = "
 LET L = 3
 MULTIPLY L BY 3
 DISPLAY TRIPLEDIGITS L
 AT 4 18
 PRINT "3 x  4 = "
 LET L = 3
 MULTIPLY L BY 4
 DISPLAY TRIPLEDIGITS L
 AT 5 18
 PRINT "3 x  5 = "
 LET L = 3
 MULTIPLY L BY 5
 DISPLAY TRIPLEDIGITS L
 AT 6 18
 PRINT "3 x 32 = "
 LET L = 3
 MULTIPLY L BY 32
 DISPLAY TRIPLEDIGITS L
 AT 7 18
 PRINT "3 x 64 = "
 LET L = 3
 MULTIPLY L BY 64
 DISPLAY TRIPLEDIGITS L
 WAITKEY

;--------------------
; Test addition
;--------------------
 CLS
 AT 0 1
 PAPER 1
 INK 2
 PRINT "CALCULATIONS TEST:            "
 AT 1 1
 PRINT "Addition:       Subtraction:  "

 PAPER 0
 INK 2

 AT 2 1
 PRINT "12 +   1 = "
 LET L = 12
 ADD 1 TO L
 DISPLAY TRIPLEDIGITS L
 AT 3 1
 PRINT "12 +  31 = "
 LET L = 12
 ADD 31 TO L
 DISPLAY TRIPLEDIGITS L
 AT 4 1
 PRINT "12 +  62 = "
 LET L = 12
 ADD 62 TO L
 DISPLAY TRIPLEDIGITS L
 AT 5 1
 PRINT "12 +  94 = "
 LET L = 12
 ADD 94 TO L
 DISPLAY TRIPLEDIGITS L
 AT 6 1
 PRINT "12 + 128 = "
 LET L = 12
 ADD 128 TO L
 DISPLAY TRIPLEDIGITS L
 AT 7 1
 PRINT "12 + 199 = "
 LET L = 12
 ADD 199 TO L
 DISPLAY TRIPLEDIGITS L

;--------------------
; Test subtration
;--------------------
