EVENT PLAYER
; SPRITEINK 6
IF SCREEN 0
    ; ASM 201
ELSE
    IF KEY UP
        IF CANGOUP
            IF D = 1
                SPRITEUP
                IF Y <= 2
                    SCREENUP
                    LET Y = 128
                    LET O = Y
                    LET P = X
                ENDIF
            ENDIF
        ENDIF
    ELSE
        IF CANGODOWN
            IF E = 1
                IF K = 0
                    SPRITEDOWN
                    IF Y > = 128
                        SCREENDOWN
                        LET Y = 2
                        LET O = Y
                        LET P = X
                    ENDIF
                ENDIF
            ENDIF
        ENDIF
    ENDIF
    IF KEY DOWN
        IF CANGODOWN
            IF E = 1
                SPRITEDOWN
                IF Y > = 128
                    SCREENDOWN
                    LET Y = 2
                    LET O = Y
                    LET P = X
                ENDIF
            ENDIF
        ENDIF
    ENDIF
    IF KEY LEFT
        IF CANGOLEFT
            IF F = 1
                LET H = 1
                SPRITELEFT
                IF X <= 2
                    SCREENLEFT
                    LET X = 216
                    LET O = Y
                    LET P = X
                ENDIF
            ENDIF
        ENDIF
    ENDIF
    IF KEY RIGHT
        IF CANGORIGHT
            IF G = 1
                SPRITERIGHT
                IF X > 216
                    SCREENRIGHT
                    LET X = 2
                    LET O = Y
                    LET P = X
                ENDIF
                LET H = 0
            ENDIF
        ENDIF
    ENDIF
    IF KEY FIRE
        IF I > = 100
            LET I = 0
            SPAWN 2 3
        ENDIF
    ENDIF
    IF A > = 8
        IF CANGODOWN
            IF E = 1
                SPRITEDOWN
                LET A = 0
                IF Y > = 128
                    SCREENDOWN
                    LET Y = 2
                    LET O = Y
                    LET P = X
                ENDIF
            ENDIF
        ENDIF
    ENDIF
ENDIF
